{
  "config": {
    "url": "http://localhost:8080/auth",
    "realm": "Urtic",
    "clientId": "UrticUI"
  },
  "initOptions": {
    "onLoad": "login-required",
    "checkLoginIframe": true
  },
  "enableBearerInterceptor": true,
  "bearerPrefix": "Bearer",
  "bearerExcludedUrls": [
    "/assets",
    "/clients/public"
  ]
}